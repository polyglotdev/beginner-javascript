{
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "jsx-quotes": 2,
    "keyword-spacing": 2,
    "prefer-object-spread": 2,
    "quotes": 2,
    "semi": "off",
    "no-var": "off",
    "no-unused-vars": "off",
    "prefer-const": "off"
  },
  "env": {
    "browser": true
  },
  "extends": [
    "wesbos",
    "prettier"
  ]
}